\begin_layout Enumerate
Within 15 km of existing HPS villages 
\end_layout

\begin_layout Enumerate
Less than 50 households with solar and less than 50 households with biomass
 electricity 
\end_layout

\begin_layout Enumerate
Less than 25% of households with grid electricity 
\end_layout

\begin_layout Enumerate
At least one tola (neighborhood) in village without grid connections 
\end_layout

